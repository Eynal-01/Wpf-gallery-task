<Window x:Class="Wpf_gallery_task.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_gallery_task"
        mc:Ignorable="d"
        Title="Gallery" Height="650" Width="1200" WindowStartupLocation="CenterScreen">
    <Grid>
        <Canvas>
            <Menu>
                <MenuItem Header="File" FontSize="20">
                    <MenuItem Header="New" FontSize="20">
                        <MenuItem.Icon>
                            <Image Source="Icons/newfile.png"></Image>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Open" FontSize="20">
                        <MenuItem.Icon>
                            <Image Source="Icons/open.png"></Image>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Save" FontSize="20">
                        <MenuItem.Icon>
                            <Image Source="Icons/save.png"></Image>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Save as" FontSize="20">
                        <MenuItem.Icon>
                            <Image Source="Icons/saveas.png"></Image>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Exit" FontSize="20">
                        <MenuItem.Icon>
                            <Image Source="Icons/exit.png"></Image>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem>
                        <MenuItem Header="Edit" FontSize="20">
                            <MenuItem Header="Add File" FontSize="20">
                                <MenuItem.Icon>
                                    <Image Source="Icons/addfile.png"></Image>
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="Add Folder" FontSize="20">
